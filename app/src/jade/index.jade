extends ./views/_layout.jade
block vars
	- var pageTitle = 'Home'
	- var bodyClass = 'home'

block content

	.c-banner
		.c-banner--background
		.o-container
			.c-banner--left
				h1.c-banner__title Welcome to Stuboki world.
				.c-banner--url__line
					a(href="#") Sign Up

			.c-banner--right
					a(href="#").c-banner--detail
						span.c-banner--url__text Created by
						span.c-banner--url__line @vanquocbui
						.c-avatar--small
							img(src="./assets/img/quoc.jpg", alt="")

	.c-breadcrumb
		.o-container
			ul.c-breadcrumb--list
				li.c-breadcrumb__item.active
					a(href="").active Recommend
				li.c-breadcrumb__item
					a(href="") Trending
				li.c-breadcrumb__item
					a(href="") Recent

	.c-grid
		.o-container
			.c-grid--row
				.c-grid--col
					.c-card
						.c-card__header
							a(href="").c-card__header-link.o-block.o-flex--cell
								.c-avatar--small
									img(src="./assets/img/john.jpeg", alt="")
								.c-card__user @johny93
							ul.o-block
								.c-button--cirkel.c-button__follow
									a(href="")
										i.mdi.mdi-plus
								.c-card__icon
									a(href="")
										i.mdi.mdi-star-outline
						.c-card__body
							a(href="")
								img(src="./assets/img/banner1.jpg", alt="")
